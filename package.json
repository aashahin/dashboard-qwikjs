{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --host --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en,ar --assetsPath=i18n",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --host --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@angular/compiler": "^17.3.10",
    "@angular/compiler-cli": "^17.3.10",
    "@builder.io/partytown": "^0.10.2",
    "@builder.io/qwik": "^1.5.5",
    "@builder.io/qwik-city": "^1.5.5",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/bun": "latest",
    "@types/eslint": "^8.56.6",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.5.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@angular/localize": "^17.3.10",
    "@fontsource/open-sans": "^5.0.28",
    "@modular-forms/qwik": "^0.24.0",
    "@qwikest/icons": "^0.0.13",
    "@types/markdown-it": "^14.1.1",
    "@types/rtl-detect": "^1.0.3",
    "@valibot/i18n": "^0.13.0",
    "apexcharts": "^3.49.1",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "event-source-plus": "^0.0.6",
    "flowbite": "^2.3.0",
    "js-cookie": "^3.0.5",
    "markdown-it": "^14.1.0",
    "qwik-sonner": "^0.4.1",
    "qwik-speak": "^0.22.1",
    "rtl-detect": "^1.1.2",
    "tailwind-merge": "^2.3.0",
    "valibot": "^0.30.0"
  },
  "module": "index.ts"
}